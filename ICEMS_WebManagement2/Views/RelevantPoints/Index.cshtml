@model IEnumerable<ICEMS_WebManagement2.RelevantPoints>

@{
    ViewBag.Title = "Index";
}

<h2>Index RelevantPoints</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.pointname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pointtype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.plantid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.machineid)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.pointname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pointtype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.plantid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.machineid)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
